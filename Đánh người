    ["Skip"] = {
        ["Avoid V4"] = false,
        ["Fruit"] = {
            ["Enabled"] = true,
            ["Avoid Fruit"] = {
                "Portal-Portal",
                "Kitsune-Kitsune",
                "Leopard-Leopard"
            }
        }
    },
    ["Spam All Skill On V4"] = {
        Enabled = true,
